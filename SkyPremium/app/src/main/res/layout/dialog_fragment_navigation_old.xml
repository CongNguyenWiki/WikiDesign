                  android:textSize="10sp"
                  app:layout_constraintEnd_toStartOf="@+id/tvLegal"
                  app:layout_constraintHorizontal_bias="0.5"
                  app:layout_constraintStart_toStartOf="parent"
                  app:layout_constraintTop_toBottomOf="@+id/tvAbout" />

              <TextView
